# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = false

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

# MSVC project files
[*.{csproj,vcxproj,pssproj,vcxproj.filters,vcxitems,targets,props}]
end_of_line = crlf
insert_final_newline = false

[*.{ps1,sln}]
indent_style = tab
indent_size = 4

[*.sln]
end_of_line = crlf
